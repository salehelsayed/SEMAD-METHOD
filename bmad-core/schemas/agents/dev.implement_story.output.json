{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "agents/dev.implement_story.output.json",
  "title": "Dev Implement Story Output",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "success": { "type": "boolean" },
    "summary": { "type": "string" },
    "files": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "path": { "type": "string", "minLength": 1 },
          "content": { "type": "string" }
        },
        "required": ["path"]
      }
    }
  },
  "required": ["success", "files"]
}

